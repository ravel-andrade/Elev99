@model Elev99.Models.CollectedData

@{
    ViewData["Title"] = "Deletar";
}

<h2>Deletar</h2>

<h3>Tem certeza que irá deletar isso?</h3>
<div>
    <h4> </h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           Andar
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Floor)
        </dd>
        <dt>
            Elevador
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Elevator)
        </dd>
        <dt>
            Turno
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Shift)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Deletar" class="btn btn-default" /> |
        <a asp-action="Index">Voltar</a>
    </form>
</div>
